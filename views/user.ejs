    <title><%= user ? user.UserFamily + ' ' + user.UserName + ' ' + user.UserLastName : 'No user' %></title>
    <link rel="stylesheet" href="/public/css/home.css">
    <% include blocks/title %>
</head>

<body>
    <% include blocks/loader %>
    <% include blocks/header %>
    <section class="app-container">
        <div class="container">
            <div class="row">
                <%- include('blocks/navigation-area', {page:"users"}) %>
                <div class="working-area">
                    <%- user ? include('blocks/userView', {user: user}) : 'Такого пользователя не существует' %>
                </div>
            </div>
    </section>
    <% include blocks/footer %>
</body>
<% include blocks/socket %>
<script src="/public/js/animateCSS.js"></script>
<script src="/public/js/home.js"></script>
