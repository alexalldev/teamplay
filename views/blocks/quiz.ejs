<div class="quiz">
    <div class="row">
        <div class="quiz-col-40">
            <img src="https://picsum.photos/100/100" class="mt-1 ml-1 img-fluid rounded" alt="">
        </div>
        <div class="text-center mt-1 quiz-col-55">
            <span class="text-dark h5"><%= quiz.games.GameName%></span>
        </div>
    </div>
    <div class="row">
        <div class="quiz-col-40">
            <div class="row">
                <div class="col-md-12 text-left">
                    <span><i class="fas fa-clock ml-1 quiz-i"></i> 15.3s</span>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 text-left">
                    <span class="text-dark h5"><i class="fas fa-question ml-1 quiz-i"></i>
                        <%= quiz.question%></span>
                </div>
            </div>
        </div>
        <div class="quiz-col-55">
            <div class="row mt-3">
                <div class="col-md-12 text-right">
                    <span class="text-dark h5 mr-1"><%= quiz.Timestamp%></span>
                </div>
            </div>
        </div>
    </div>
</div>